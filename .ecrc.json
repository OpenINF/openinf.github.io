{
  "Verbose": false,
  "Debug": false,
  "IgnoreDefaults": true,
  "SpacesAftertabs": false,
  "NoColor": false,
  "Exclude": [
    "(.devcontainer)",
    "(.github)",
    "(.vscode)",
    "(doc)",
    "(lib)",
    "vendor/",
    "\\.md$",
    "\\.mjs$",
    "pnpm-lock.yaml",
    "Gemfile.lock",
    ".markdownlint.jsonc",
    "LICENSE/"
  ],
  "AllowedContentTypes": [],
  "PassedFiles": [],
  "Disable": {
    "EndOfLine": false,
    "Indentation": false,
    "InsertFinalNewline": false,
    "TrimTrailingWhitespace": false,
    "IndentSize": false,
    "MaxLineLength": false
  }
}
